# shellcheck shell=sh disable=SC2039,SC2142,SC3043

xrc .x-cmd/testcases/v0_test
ls_subcmd(){
    param:dsl <<A
subcommand:
    aaa                     "This is a aaa"
    bbb                     "This is a bbb"
    ccc                     "This is a ccc"
A
    param:run

}

test_ls_subcmd(){
    @assert stdout ls_subcmd _ls_subcmd<<A
aaa
This is a aaa
bbb
This is a bbb
ccc
This is a ccc
A
}