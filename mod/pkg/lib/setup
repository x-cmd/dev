
___x_cmd_pkg_setup(){
    # If not installed, setup
    ___x_cmd_pkg_setup___min "$@"
}

___x_cmd_pkg_setup___min(){
    local script_path
    pkg:debug "$(___x_cmd_pkg___get_script_path "$@" "" setup)"
    if script_path="$(___x_cmd_pkg___get_script_path "$@" "" setup)"; then
        . "$___X_CMD_PKG_RAWPATH$script_path"
    else
        :
    fi
}
